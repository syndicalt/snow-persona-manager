	// var today = date.getFullYear() + "-" + date.getMonth() + "-" + date.getDate() + " 00:00:00";
	// g_form.setValue("end_date", today);

	gsftSubmit(null, g_form.getFormElement(), "end_session");	
}

if(typeof window == "undefined") {
	serverEndSession();
}

function serverEndSession() {
	var today = new GlideDateTime();
	current.end_date = today;
	current.active = false;
	current.notes += "Session ended via UI Action.";
	current.update();

	var scriptGr = new GlideRecord("sysauto_script");
	scriptGr.addEncodedQuery("nameINDaily: Maintain User Sessions (Existing User) - End,Daily: Maintain User Sessions (Persona User) - End");
	scriptGr.query();

	while(scriptGr.next()) {
		gs.executeNow(scriptGr);
	}

	action.setRedirectURL(current);
}]]></script>
