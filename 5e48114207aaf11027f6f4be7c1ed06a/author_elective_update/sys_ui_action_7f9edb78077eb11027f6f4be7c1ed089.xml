        <order>100</order>
        <script><![CDATA[function endSessions() {
	var sessions = "";

	if(typeof g_list != "undefined") { 
		sessions = g_list.getChecked();
	}

	var ga = new GlideAjax("personaManagerClientHandler");
	ga.addParam("sysparm_name", "endSessionsFromList");
	ga.addParam("sysparm_sessionsList", sessions);
	ga.getXML(function(response) {
		var answer = JSON.parse(response.responseXML.documentElement.getAttribute("answer"));
		
		if(answer.status == 400) {
			alert("An error has occurred: " + answer.message);
			return false;
		}
		
		alert(answer.message);
		return true;
	});
}]]></script>
