        <name>End Session(s) Now</name>
        <onclick>endSessions()</onclick>
        <order>100</order>
        <script><![CDATA[function endSessions() {
	var sys_id = "";
		
	if(typeof g_sysId == "undefined") {
		if(g_list.getChecked() == "") {
			g_list.addInfoMessage(getMessage("Select a record to end session."));
			return;
		}
	} else {
		sys_id = g_sysId;
	}

	if(sys_id == "")	{
		var checked = g_list.getChecked();
		var checkedSplit = checked.split(',');

		if(checkedSplit.length > 1) {
			alert(getMessage("Select a single record to delete."));
			return;
		}

		sys_id = checkedSplit[0];
	}

	var answer = confirm(getMessage("Do you want to end these session(s)?"));
	
	if(answer === true) {
		alert(sys_id);
	}
	else {
		return false; 
	}
}

