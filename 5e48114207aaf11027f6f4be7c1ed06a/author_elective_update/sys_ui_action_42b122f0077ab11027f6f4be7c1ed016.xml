        <name>End Session(s) Now</name>
        <onclick>endSessions()</onclick>
        <order>100</order>
        <script><![CDATA[function endSessions() {
	var sys_id;
	
	if (typeof g_sysId != 'undefined' && g_sysId) {
		sys_id = g_sysId;
	} else if (typeof g_list != 'undefined') {
		sys_id = g_list.getChecked();
	} else {
		sys_id = g_form.getUniqueValue();
	}

	sys_id = sys_id.split(','); 

	sys_id.forEach(function(rec) {
		var gd = Date.now();
		var gr = new GlideRecord("x_975207_persona_m_persona_sessions");
		gr.get(rec);
		gr.end_date = gd;
		gr.update();
	});

	var scriptGr = new GlideRecord("sysauto_script");
	scriptGr.addEncodedQuery("nameINDaily: Maintain User Sessions (Existing User) - End,Daily: Maintain User Sessions (Persona User) - End");
	scriptGr.query();
	
	while(scriptGr.next()) {
		if (typeof SncTriggerSynchronizer != 'undefined') {
			SncTriggerSynchronizer.executeNow(scriptGr);
		} else {
			Packages.com.snc.automation.TriggerSynchronizer.executeNow(scriptGr);
		}
	}
}]]></script>
